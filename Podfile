# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'findCVS' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for findCVS

pod 'RxSwift'
pod 'RxCocoa'
pod 'SnapKit'

  target 'findCVSTests' do
    inherit! :search_paths
    # Pods for testing
pod 'Stubber'
pod 'Nimble'
pod 'RxBlocking'
pod 'RxTest'
  end

end
